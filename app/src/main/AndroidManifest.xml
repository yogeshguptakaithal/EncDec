<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.EncDec">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logf1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.EncDec.User2decrypt">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name="com.EncDec.decrypt">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name="com.EncDec.Encrypt">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name="com.EncDec.allowedlist">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name="com.EncDec.loginsingnup">
            android:screenOrientation="portrait"

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />


                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.EncDec.MainActivity">
            android:screenOrientation="portrait"
        </activity>
    </application>

</manifest>